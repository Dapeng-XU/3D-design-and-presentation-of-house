{
  "filetype": "pagedata",
  "object-class": "camera",
  "operation": "set-parameter",
  "content": {
    "title": "游览视角观察",
    "items": [
      {
        "chsname": "你视线的高度",
        "typename": "configuration",
        "datatype": "number",
        "min": 0,
        "max": 1000,
        "step": 0.1,
        "precision": 1,
        "varible": "hCamera.tourist.height"
      },
      {
        "chsname": "游走速度（距离）",
        "typename": "configuration",
        "datatype": "number",
        "min": 0,
        "max": 10,
        "step": 0.1,
        "precision": 1,
        "varible": "hCamera.step"
      },
      {
        "chsname": "你旋转步伐的快慢",
        "typename": "configuration",
        "datatype": "number",
        "min": 0,
        "max": 10,
        "step": 0.0004,
        "precision": 4,
        "varible": "hCamera.tourist.rotationStep"
      }
    ]
  }
}
